{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsx --watch --no-cache src/index.ts",
    "build": "tsc && cp -r prisma build/ && cd ./build && npx prisma generate && mv ./src/generated . && rm -rf ./src",
    "start": "node build/index.js"
  },
  "keywords": [],
  "author": "Jonathanleivag TCIT",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "5.0.1",
    "@types/node": "22.15.1",
    "prisma": "6.6.0",
    "ts-standard": "12.0.2",
    "tsx": "4.19.3",
    "typescript": "5.8.3"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "build/*"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "./node_modules/ts-standard/eslintrc.json"
    ]
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "cors": "2.8.5",
    "dotenv": "16.5.0",
    "express": "5.1.0",
    "helmet": "8.1.0"
  }
